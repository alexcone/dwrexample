<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.g4g.dto">
	<class name="NationalCodeDTO" table="nationalcode">
		<id name="id" column="natcodeid">
			<generator class="increment" />
		</id>
		<property name="islegal" />
		<property name="nationcode" unique="true" not-null="true"/>
		<property name="nationname" not-null="true" />
	</class>
</hibernate-mapping>