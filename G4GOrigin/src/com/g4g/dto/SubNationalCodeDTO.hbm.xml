<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.g4g.dto">
	<class name="SubNationalCodeDTO" table="subnationalcode">
		<id name="id" column="subnatcodeid">
			<generator class="increment" />
		</id>
		<property name="islegal" />
		<property name="natcodeid" />
		<property name="nationcode" />
		<property name="localabbrev" />
		<property name="nationname" />
	</class>
</hibernate-mapping>